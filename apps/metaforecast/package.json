{
  "name": "metaforecast",
  "version": "2.0.0",
  "private": true,
  "description": "Get forecasts from various platforms",
  "engines": {
    "node": "22.x"
  },
  "keywords": [
    "forecasts",
    "predictions"
  ],
  "author": "Nu√±o Sempere",
