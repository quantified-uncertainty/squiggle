  "scripts": {
    "cli": "NODE_NO_WARNINGS=1 tsx src/backend/cli/index.ts",
    "setCookies": "./src/backend/manual/setCookies.sh",
    "dev": "next dev -p 3002",
    "gen:schema": "tsx src/graphql/print-schema.ts",
    "gen:gql": "graphql-codegen",
    "gen": "pnpm run gen:schema && pnpm run gen:gql",
    "build:ts": "pnpm gen && tsc",
    "build": "pnpm run gen && next build",
    "dbshell": ". .env && psql $METAFORECAST_DB_URL",
    "lint": "pnpm lint:prettier && pnpm eslint",
    "lint:prettier": "prettier --check .",
    "eslint": "ESLINT_USE_FLAT_CONFIG=false NODE_NO_WARNINGS=1 eslint src",
    "format": "prettier --write ."
  },
